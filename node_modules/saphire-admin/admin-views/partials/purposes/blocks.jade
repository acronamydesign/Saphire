div.panel.clearfix
	div.panel-body.col-md-6
		h4 Themes
		select.form-control
			option theme
			option theme 2

	div.panel-body.col-md-3.col-md-offset-3
		h4 Filter
		input.form-control(type="search" placeholder="filter")

ul.list-group
	each region in regionsList
		li.list-group-item
				h4 #{region}
